    "import yfinance as yf\n",
    "import pandas as pd\n",
    "pd.set_option('display.max_columns', None)\n",
    "\n",
    "recent_data=yf.download(stock,period=\"3mo\", interval='60m')  \n",
    "print(f'\\n***************{stock_name}***************\\n')\n",
    "recent_data"
